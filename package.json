{
  "name": "jaketreacher-website",
  "version": "1.0.0",
  "description": "Jake Treacher Personal Website",
  "scripts": {
    "watch:sass": "sass --watch -I node_modules/bulma src/_assets/styles:_site/css",
    "build:sass": "sass -I node_modules/bulma src/_assets/styles:_site/css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*",
    "clean": "rm -rf _site"
  },
  "author": "Jake Treacher",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.43.2"
  },
  "dependencies": {
    "bulma": "^0.9.3"
  }
}